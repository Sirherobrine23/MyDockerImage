{
  "name": "Docker daemon",
  "process": {
    "restart": "always",
    "restartCount": 3,
    "waitSeconds": 10,
    "command": "dockerd",
    "args": [
      "--experimental"
    ]
  },
  "dependecies": [
    {
      "name": "K3d Kubernetes",
      "process": {
        "restart": "no",
        "command": "bash",
        "args": [
          "-c",
          "k3d cluster create node1 && chmod 777 $KUBECONFIG"
        ]
      }
    }
  ]
}